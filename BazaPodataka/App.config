<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.7.2" />
    </startup>
	<system.serviceModel>
		<services>
			<service name="BazaPodataka.BazaPodatakaService">
				<host>
					<baseAddresses>

						<add baseAddress="net.tcp://localhost:4003" />
					</baseAddresses>

				</host>
				<!-- Service Endpoints -->

				<endpoint address="ServiceBaza" binding="netTcpBinding" contract="Common.IBazaPodataka" />
			</service>

		</services>
	</system.serviceModel>
</configuration>